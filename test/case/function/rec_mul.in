let is_odd = \(n){
    if n = 0 {
        return false;
    }else{
        return is_even(n-1);
    }
};
let is_even = \(n){
    if n = 0 {
        return true;
    }else{
        return is_odd(n-1);
    }
};

let unit = print(is_odd(20));
let unit = print(is_even(20));
let unit = print(is_odd(3));
let unit = print(is_even(3));
