let true = 1=1;
let false = 1/=1;

let pow = \(x,n){
    if n = 0 then{
        return 1;
    }else{
        let r = pow(x*x,n/2);
        if n % 2 = 1 then{
            return r * x;
        }else{
            return r;
        }
    }
};
let gcd = \(a,b){
    if b = 0 then{
        return a;
    }else{
        return gcd(b,a%b);
    }
};
let lcm = \(a,b){
    return a*b / gcd(a,b);
};
